<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kirgu_tsd.ru" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Киргу ТСД</name>
    <description>
        Приложение для сотрудников компании киргу.
    </description>
    <author email="umakhanr7@yandex.ru" href="mailto:umakhanr7@yandex.ru">Umakhan Magomedov</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <icon src="www/icon.png" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="fullscreen" value="false" />
    <preference name="orientation" value="portrait" />
    <preference name="BackgroundColor" value="0xffffffff" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="SplashScreenBackgroundColor" value="0xffffffff" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="android-minSdkVersion" value="22" />
    <preference name="android-targetSdkVersion" value="29" />
    <preference name="InterceptRemoteRequests" value="all" />
</widget>
